view.v-button {
  background: #fff;
  width: 750rpx;
  height: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 10;
  font-size: 32rpx;
  border-top: 2rpx solid #eee;
  background: #FFDB45;
  color: #fff;
}
.right-btn {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  color: #C9C9C9;
  font-size: 24rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.right-btn > text {
    margin-top: -5rpx;
}
.ybtn {
  background-color: #FFDA44;
  border-radius: 10rpx;
  font-size: 24rpx;
}
.ybtn::after {
    border: none;
}
.col {
  margin-top: 40rpx;
}
.col .btnhover {
    color: #F1F1F1 !important;
}
.col .uni-form-item {
    background: #fff;
    padding: 20rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
}
.col .uni-form-item .title {
      width: 180rpx;
}
.col .uni-form-item .uni-input {
      width: 510rpx;
}
.col .uni-btn-v {
    margin: 100rpx 40rpx 0;
}
.col .uni-btn-v button {
      background-color: #FFDB45 !important;
      color: #fff;
      margin-bottom: 20rpx;
}
.col .uni-btn-v button::after {
        border: none;
}
.col .uni-btn-v button.del-btn {
        background: #eee !important;
        color: #333;
}
.col .uni-btn-v button.del-btn::after {
          border: 2rpx solid #ccc;
}

